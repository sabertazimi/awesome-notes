{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "notes/**/*"],
  "exclude": ["node_modules", "build", "dist"]
}
